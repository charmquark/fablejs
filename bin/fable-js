#!/usr/bin/env ruby
# The command line FableJS driver.

THIS_FILE = File.symlink?(__FILE__) ? File.readlink(__FILE__) : __FILE__
begin
	require File.dirname(THIS_FILE) + '/../lib/fable_js'
rescue LoadError
	require 'fable_js'
end
require 'fable_js/exec'

program = FableJS::Exec::Program.new(ARGV)
program.run
